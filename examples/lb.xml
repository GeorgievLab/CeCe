    <module name="cell.generator" object="cell.Yeast" probability="0.01" />
    <module name="streamlines" grid="100" velocity-inflow="200um/s" viscosity="0.653mPas" />
    <object class="cell.Cell" type="static" volume="2000um3" />
    <!--object class="cell.Cell" volume="200um3" position="-20um 0" /-->
    <object class="cell.Yeast" position="-20um 0" adensity="0.01" />
    <object class="cell.Yeast" position="-20um 30um" adensity="0.01" />
</simulation>

