<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<!-- Simulation definition -->
<simulation world-size="100um 100um" dt="1ms" iterations="10000">
    <!-- Generate yeast cells -->
    <module name="object-generator">
        <object class="cell.Yeast" probability="0.01" />
    </module>

    <!-- Define streamlines -->
    <module name="streamlines" grid="100" velocity-inflow="200um/s" kinematic-viscosity="0.658um2/s" />

    <!-- Center cell -->
    <object class="cell.Cell" type="static" volume="2000um3" />
</simulation>

